#mySlider.carousel.slide(data-ride="carousel").mb-0.mr-0.sliderStyle

    ol.carousel-indicators
        li(data-target="#mySlider" data-slide-to="1")
        li(data-target="#mySlider" data-slide-to="2")

    .carousel-inner
        .carousel-item.active
            img(src="/img/g8.png" alt="").d-block.img-fluid
        
        .carousel-item
            img(src="/img/g12.png" alt="").d-block.img-fluid


    a(href="#mySlider"  data-slide="prev").carousel-control-prev
        span.carousel-control-prev-icon.bg-dark
    
    a(href="#mySlider"  data-slide="next").carousel-control-next
        span.carousel-control-next-icon.bg-dark

